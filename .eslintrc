{
	"parser": "babel-eslint",
	"env":{
		"browser":true,
		"node":true,
		"es6":true,
		"mocha":true
	},
	"rules": {
		"strict":0,
		"eqeqeq":0,
		"quotes":0,
		"comma-spacing":1,
		"no-unused-vars":0,
		"no-ternary":1,
		"no-path-concat":0,
		"no-trailing-spaces": 1,
		"key-spacing": 1,
		"no-unused-expressions": 1,
		"no-underscore-dangle": 1,
		"new-cap":1
	},
	"ecmaFeaturesj": {
		"jsx":true,
		"spread":true,
		"generators":true
	}
}
